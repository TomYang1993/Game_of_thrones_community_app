<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Leaflet Zoomify Demo</title>
    <link rel="stylesheet" href="https://unpkg.com/leaflet@0.7.7/dist/leaflet.css" />
    <style type="text/css">
        html, body, #map {
            width: 100%;
            height: 100%;
        }
    </style>
</head>

<body>

    <div id="map">

    </div>



    <script src="https://unpkg.com/leaflet@0.7.7/dist/leaflet.js"></script>

    <script type="text/javascript">

    var map = L.map('map', {

        zoom: 4
        });
    L.tileLayer('http://localhost:3000/assets/tiles/{z}/{y}/{x}.png',{tms: true}).addTo(map);

    </script>
</body>
</html>
